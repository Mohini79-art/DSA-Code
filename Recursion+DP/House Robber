class Solution {
    public int maxrob(int[] arr,int idx,int []dp) {
        if(idx>=arr.length){
            return 0;
        }
        if(dp[idx]!=-1){
            return dp[idx];
        }
        int steal=arr[idx]+maxrob(arr,idx+2,dp);
        int skip=maxrob(arr,idx+1,dp);
        return dp[idx]=Math.max(skip,steal);
    }
    public int rob(int[] arr) {
        int []dp=new int [arr.length];
        Arrays.fill(dp,-1);
        return maxrob(arr,0,dp);
    }
}
