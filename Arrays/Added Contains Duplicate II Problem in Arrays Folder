class Solution {
    public boolean containsNearbyDuplicate(int[]arr, int k) {
        HashSet<Integer>set=new HashSet<>();
        for(int i=0;i<arr.length;i++){
           if(set.contains(arr[i])){
            return true;
           }     
           set.add(arr[i]);
           if(set.size()>k){
            set.remove(arr[i-k]);
           }
        }
        return false;
    }
}
