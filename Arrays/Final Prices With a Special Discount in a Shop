class Solution {
    public int[] finalPrices(int[] arr) {
        int res[]=new int [arr.length];
        for(int i=0;i<arr.length;i++){
            int cost=arr[i];
            for(int j=i+1;j<arr.length;j++){
                if(arr[j]<=cost){
                    cost=arr[i]-arr[j];
                    break;
                }
            }
            res[i]=cost;
        }
        return res;
    }
}
