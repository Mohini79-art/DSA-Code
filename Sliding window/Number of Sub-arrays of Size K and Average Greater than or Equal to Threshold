class Solution {
    public int numOfSubarrays(int[] arr, int k, int threshold) {
        int sum=0;
        int average=0;
        int ans=0;
        for(int i=0;i<k;i++){
            sum+=arr[i];
        }
        average=sum/k;
        if(average>=threshold){
            ans++;
        }
        for(int i=k;i<arr.length;i++){
            sum+=arr[i]-arr[i-k];
            average=sum/k;
            if(average>=threshold){
            ans++;
        }
        }
        return ans;
    }
}
